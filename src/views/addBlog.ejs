<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include('./partials/head') %>
		<title>Home</title>
	</head>

	<body>
		<%- include('./partials/nav') %>
		<div class="container mt-3">
			<form action="/blog" method="post">
				<div class="mb-3">
					<label for="title" class="form-label">Title</label>
					<input
						type="text"
						class="form-control"
						id="title"
						name="title"
						aria-describedby="title"
					/>
				</div>
				<div class="mb-3">
					<label for="coverImage" class="coverImage">CoverImage</label>
					<input
						type="file"
						class="form-control"
						id="coverImage"
						name="coverImage"
						aria-describedby="coverImage"
					/>
				</div>
				<div class="mb-3">
					<label for="body">Body</label>
					<textarea
						class="form-control"
						id="body"
						style="height: 100px"
					></textarea>
				</div>
				<button class="btn btn-info">Submit</button>
			</form>
		</div>
		<%- include('./partials/scripts') %>
	</body>
</html>
